<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.addStockController">
   <children>
      <Label layoutX="270.0" layoutY="14.0" text="ADD STOCK">
         <font>
            <Font size="30.0" />
         </font>
      </Label>
      <TableView fx:id="wTable" layoutX="87.0" layoutY="82.0" prefHeight="365.0" prefWidth="527.0">
        <columns>
          <TableColumn fx:id="col_wid" prefWidth="75.0" text="WID" />
          <TableColumn fx:id="col_pin" prefWidth="75.0" text="PINCODE" />
            <TableColumn fx:id="col_wqi" prefWidth="75.0" text="WQI" />
            <TableColumn fx:id="col_name" prefWidth="75.0" text="NAME" />
            <TableColumn fx:id="col_capacity" prefWidth="75.0" text="CAPACITY" />
            <TableColumn fx:id="col_remcap" prefWidth="75.0" text="REMCAP" />
            <TableColumn fx:id="col_cost" prefWidth="75.0" text="COST" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="59.0" text="Please Select a warehouse: ">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Button fx:id="selBtn" layoutX="148.0" layoutY="454.0" mnemonicParsing="false" onAction="#selectWarehouse" text="Select Warehouse" />
      <Button fx:id="backBtn" layoutX="440.0" layoutY="454.0" mnemonicParsing="false" onAction="#back" text="Back" />
      <Label fx:id="debugLabel" layoutX="206.0" layoutY="63.0" textFill="#e41d1d" />
   </children>
</AnchorPane>
